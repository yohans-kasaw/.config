{
    "position": "top",
    "height": 30,
    "spacing": 4,
    "modules-right": [
        "group/tray-group",
        "group/settings-group",
        "group/metrics-group"
    ],
    "modules-center": [
        "hyprland/workspaces"
    ],
    "modules-left": [
        "clock#date"
    ],
    "hyprland/workspaces": {
        "disable-scroll": true,
        "warp-on-scroll": false,
        "format": "{icon}",
        "format-icons": {
            "1": "Û∞≤†",
            "2": "Û∞≤¢",
            "3": "Óûï",
            "4": "ÔçØ",
            "6": "Û∞ô®",
            "5": "Û∞àπ",
            "7": "Ôíä",
            "8": "Û∞í±",
            "9": "Ôàõ ",
            "10": "Û±ö•",
            "11": "Ôà≥",
            "urgent": "Û±à∏",
            "default": "ÔÑë"
        },
        "active-only": false,
        "all-outputs": false
    },
    "clock#time": {
        "format": "{:%I:%M %p}"
    },
    "clock#date": {
        "format": "{:%I:%M %p, %a, %d %b %Y}"
    },
    "custom/gpu": {
        "exec": "/home/yohansh/config/waybar/scripts/gpu_usage.sh",
        "format": "gpu {}",
        "return-type": "json",
        "interval": 2
    },
    "group/tray-group": {
        "orientation": "inherit",
        "modules": [
            "tray"
        ]
    },
    "group/metrics-group": {
        "orientation": "inherit",
        "modules": [
            "cpu",
            "custom/gpu",
            "memory"
        ]
    },
    "group/settings-group": {
        "orientation": "inherit",
        "modules": [
            "backlight",
            "pulseaudio",
            "battery"
        ]
    },
    "battery": {
        "states": {
            "warning": 30,
            "critical": 15
        },
        "format": "<span >{icon}  </span>{capacity}%",
        "format-warning": "<span >{icon}  </span> {capacity}%",
        "format-critical": "<span >{icon}  </span> {capacity}%",
        "format-charging": "<span >Ôá¶  </span>{capacity}%",
        "format-plugged": "<span >Ôá¶  </span>{capacity}%",
        "format-alt": "<span >{icon}  </span>{time}",
        "format-full": "<span >Û±ä¶  </span>{capacity}%",
        "format-icons": [
            "ÔâÑ",
            "ÔâÉ",
            "ÔâÇ",
            "ÔâÅ",
            "ÔâÄ"
        ],
        "tooltip-format": "{time}"
    },
    "pulseaudio": {
        "format": "{icon} {volume}%",
        "format-muted": "üîá",
        "format-icons": {
            "headphones": "Û∞ãã",
            "headset": "Û∞ãé",
            "default": [
                "Û∞ïø",
                "Û∞ñÄ",
                "Û∞ïæ"
            ]
        },
        "on-click": "pavucontrol"
    },
    "backlight": {
        "format": "{icon} {percent}%",
        "device": "intel_backlight",
        "format-icons": [
            "ÔÜÖ  ",
            "ÔÑë  "
        ]
    },
    "tray": {
        "spacing": 10
    },
    "cpu": {
        "format": "cpu {usage}%"
    },
    "memory": {
        "format": "Mem {used:0.1f}GB"
    }
}
